@Article{Halldórsson2013,
	author={Halld{\'o}rsson, Bjarni V.
				and Blokh, Dima
				and Sharan, Roded},
	title = {Estimating population size via line graph reconstruction},
	journal = {Algorithms for Molecular Biology},
	year = {2013},
	month = {Jul},
	volume = { 8},
	number = {1},
	pages = {17},
	abstract={We propose a novel graph theoretic method to estimate haplotype population size from genotype data. The method considers only the potential sharing of haplotypes between individuals and is based on transforming the graph of potential haplotype sharing into a line graph using a minimum number of edge and vertex deletions.},
	issn = {1748-7188},
	doi = {10.1186/1748-7188-8-17},
	url = {https://doi.org/10.1186/1748-7188-8-17},
	note = {l’article propose une m\'ethode de d\'ecouverte de la gen\'ealogie de population en se basant sur les haplotypes partag\'ees dans 	les genomes des individus. L’objectif \'etant d’identifier ses haplotypes communs \`a des individus, l’approche choisie est la notion de line graphes. L’utilisation de line-graphes est pertinente parce que ces haplotypes sont uniques et deux individus partageant les memes haplotypes sont li\'es par une ar\^ete. Ces haplotypes deviennent les sommets du line graphes et l’arbre gen\'ealogique est connu en utilisant le graphe racine du line graphe. 
\`A partir des individus et de leurs haloptypes, on construit un graphe dont les individus sont des sommets et les haloptypes sont des ar\^etes de ce graphe dans laquelle deux individus partageant entre cette propri\'et\'e sont reli\'es par une ar\^ete. Ce graphe prend le nom de graphe Clark Consistency graph \cite{halldorsson2011clark}. Si ce graphe est correct alors il est un line-graphe. Cependant certains haloptypes sont surperfus et certaines ar\^etes du graphe n’ayant aucune signification engendrant que le graphe ne soit pas un line-graphe. 
Ce probleme \'etant NP-Complete, la solution propos\'ee realise un algorithme de suppression de sommets et un algorithme de suppression d’ar\^etes. L’algorithme de suppression de sommets est une 6-approximation alors que celui des ar\^etes est de complexit\'e $O(n*m)$ avec $m$ le nombre de sommets et $n$ le nombre d'ar\^etes.
Dans le cas ou il realise des suppressions, le line graphe fourni est le plus proche possible du line graphe de l’arbre genealogique.
La particularit\'e de  la solution est l’absence d’ar\^etes ajout\'ees dans le line-graphe entrainant que cette solution est inapplicable dans notre probleme ou il existe des aretes inconnus dans notre graphe de correlation.}
	}

